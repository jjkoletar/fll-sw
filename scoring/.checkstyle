<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="true">
    <local-check-config name="Standard checks" location="checkstyle_checks.xml" type="project" description="">
        <property name="dir.build" value="build"/>
        <additional-data name="protect-config-file" value="false"/>
    </local-check-config>
    <local-check-config name="Strict Checks" location="checkstyle_nightly_checks.xml" type="project" description="">
        <property name="dir.build" value="build"/>
        <additional-data name="protect-config-file" value="false"/>
    </local-check-config>
    <fileset name="all" enabled="true" check-config-name="Standard checks" local="true">
        <file-match-pattern match-pattern="." include-pattern="true"/>
    </fileset>
</fileset-config>
